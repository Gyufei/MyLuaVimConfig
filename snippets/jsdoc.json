{
    "JSDoc Comment": {
        "prefix": "/*",
        "body": [
            "/**\n",
            " * ${1:Comment}$0\n",
            "*/"
        ],
        "description": "A JSDoc comment"
    },
    "varname": {
        "prefix": "@p",
        "body": [
            "@param ${1:Type} ${2:varname} ${3:Description}"
        ],
        "description": "@param {Type} varname Description"
    },
    "return type desc": {
        "prefix": "@ret",
        "body": [
            "@return ${1:Type} ${2:Description}"
        ],
        "description": "@return {Type} Description"
    }
}
