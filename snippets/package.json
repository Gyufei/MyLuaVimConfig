{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./global.json"
      },
      {
        "language": "solidity",
        "path": "./solidity.json"
      },
      {
        "language": [
          "shellscript",
          "shell",
          "sh",
          "zsh"
        ],
        "path": "./shell.json"
      },
      {
        "language": [
          "markdown",
          "rmd"
        ],
        "path": "./markdown.json"
      },
      {
        "language": [
          "html",
          "jade",
          "pug",
          "eruby",
          "javascriptreact",
          "htmldjango"
        ],
        "path": "./html.json"
      },
      {
        "language": [
          "css",
          "scss",
          "sass",
          "less",
          "stylus"
        ],
        "path": "./css.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "vue",
          "svelte"
        ],
        "path": "./javascript.json"
      },
      {
        "language": [
          "typescript",
          "typescriptreact"
        ],
        "path": "./typescript.json"
      },
      {
        "language": "vue",
        "path": "./vue/html.json"
      },
      {
        "language": "vue",
        "path": "./vue/pug.json"
      },
      {
        "language": "vue",
        "path": "./vue/javascript.json"
      },
      {
        "language": "vue",
        "path": "./vue/vue.json"
      }
    ]
  }
}
